<Window x:Class="Kinectomix.LevelEditor.View.NewLevelWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:View="clr-namespace:Kinectomix.LevelEditor.View"
        xmlns:ViewModel="clr-namespace:Kinectomix.LevelEditor.ViewModel"
        xmlns:xc="clr-namespace:ExCastle.Wpf"
        xc:DialogCloser.DialogResult="{Binding DialogResult}"
        Title="New Level" Height="300" Width="300">
    <Window.DataContext>
        <ViewModel:NewLevelViewModel/>
    </Window.DataContext>
    <Grid>
        <View:NewLevelControl/>
    </Grid>
</Window>
